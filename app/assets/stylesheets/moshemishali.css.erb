.fancybox-skin.video {
	-moz-border-radius:15px!important;
	-webkit-border-radius:15px!important;
	background-color:#000;
	border:2px outset #fff;
	border-radius:15px!important;
	color:#000;
	padding:6px
}

.fb-like {
	left: 3% !important;
	position: absolute !important;
	top: 109px !important
}

.films a.soon,a.film.soon {
	cursor:not-allowed
}

.videodetails #description {
	font-size:13pt;
	font-weight:400
}

.videodetails #title,.videodetails #description {
	display:inline;
	font-size:18pt;
	font-weight:700;
	padding-left:18px
}

.videodetails .vidout {
	text-align:justify;
	vertical-align:top
}

.videodetails iframe {
	margin:2px 20px!important
}

.videodetails_tooltip {
	-moz-border-radius: 10px!important;
	-webkit-border-radius: 10px!important;
	background: #ffffc8;
	border: 2px solid rgb(58, 58, 58);
	border-radius: 10px!important;
	color: rgb(58, 58, 58);
	font-size: 12pt;
	padding: 10px;
	position: absolute;
	width: 400px;
	z-index: 9999
}

.videodetails_tooltip #synopsis {
	padding: 10px
}

.videodetails_tooltip #description {
	color: #555555;
	font-style:italic;
	font-size: 12pt;
	display:inline-block;
}

.videodetails_tooltip #title {
	display:inline-block;
	font-size:22pt;
	font-weight: bold
}

::-webkit-scrollbar {
	width:10px
}

::-webkit-scrollbar-corner {
	background:#000
}

::-webkit-scrollbar-thumb {
	-webkit-border-radius:10px;
	-webkit-box-shadow:inset 0 0 6px rgb(0, 73, 73, 1);
	background:rgba(0, 73, 73, 1);
	border-radius:10px
}

::-webkit-scrollbar-track {
	-webkit-border-radius:10px;
	-webkit-box-shadow:inset 0 0 6px rgba(255,255,255,0.3);
	border-radius:10px
}

body {
	background-color:#000;
	color:#afafaf;
	font:12pt "times new roman", arial;
	overflow:hidden
}

div#about div.contact {
	display:inline-block;
	vertical-align:top;
	width:20%
}

div#about div.contact a.network {
	background:url(<%=asset_path"network_icons.png"%>);
	height:30px;
	opacity:0.7;
	width:30px
}

div#about div.contact a.network img {
	height:30px;
	opacity:0.8;
	width:30px
}

div#about div.contact a.network.facebook {
	background-position:-71px 0
}

div#about div.contact a.network.gmail {
	background-position:0 0
}

div#about div.contact a.network.linkedin {
	background-position:-36px 0
}

div#about div.contact a.network:hover {
	opacity:1.0
}

div#about div.contact ul {
	-webkit-padding-start:10px;
	list-style:none
}

div#about div.contact ul li {
	margin-bottom:12px
}

div#about div.contact ul li a.numb,div#about div.contact ul li a.numb:hover,div#about div.contact ul li a.numb:active {
	color:#afafaf
}

div#about div.contact ul li span {
	color:#fff;
	display:block;
	font-weight:700
}

div#about div.contact ul li.last {
	margin-bottom:0;
	width:180px
}

div#footer {
	background-image:url(<%=asset_path"bottom.jpg"%>);
	background-position:top;
	background-repeat:no-repeat;
	background-size:100%;
	bottom:0;
	color:#afafaf;
	min-height:55px;
	position:fixed;
	text-align:right;
	width:100%
}

div#footer span.footertext {
	cursor:default;
	margin-right:45px
}

ul.slider li a,
div.blurry_black {
	-webkit-box-shadow: inset 0 0 30px #000;
	-moz-box-shadow: inset 0 0 30px #000;
	box-shadow: inset 0 0 30px #000
}

div#header {
	background-image: url(/assets/top_notext.png);
	background-position: top;
	background-repeat: no-repeat;
	background-size: 100%;
	display: block;
	min-height: 127px;
	width:100%;
	margin: 0 auto;
	background-position: 0px -30px
}

div#header .header_title,
div#header .header_subtitle {
	-moz-border-radius:20px;
	-webkit-border-radius:20px;
	border-radius:20px;
	font-size: 31pt;
	color: #fff;
	box-shadow: 1px 1px 5px 20px rgba(0,116,116, 0.2) inset;
	position: absolute;
	top: 15px;
	left: 35px
}

div#header .header_subtitle {
	top: 64px;
	font-size: 21pt
}

div#header div.headerbuttons {
	height:23px;
	margin:0 auto;
	position:relative;
	top:100px;
	width:686px
}

div#header div.headerbuttons a span {
	color:transparent;
	display:block;
	font:13pt/18pt times new roman;
	font-weight:700;
	text-align:center;
	text-shadow:0 0 1px #fff
}

div#header div.headerbuttons a,div#about div.contact a.network {
	display:inline-block;
	text-decoration:none!important
}

div#main {
	margin:0 auto;
	width:90%
}

div#main div.section {
	overflow:scroll;
	overflow-x:hidden;
	overflow-y:auto;
	padding-left:3.5%;
	color:#afafaf;
	font-size:12pt;
	margin:10px auto 0!important;
	width:100%
}

div#main div.section#welcome {
	overflow: hidden;
	padding: 0;
}

div.about {
	padding-left:10%;
	width:78%
}

div.about div.about_text {
	display:inline-block;
	margin:12px 27px 0 0;
	width:54%
}

div.about img.face_img {
	display:inline-block;
	height:177px;
	padding:16px 5px 0 0;
	vertical-align:top;
	width:130px
}

div.contact li a:hover img {
	opacity:1
}

div.filmography div.sectionheader {
	font-size:18pt;
	font-weight:700;
	margin:0
}

div.filmography ul li span {
	font-size:13pt;
	font-style:italic
}

div.gallery {
	padding-left:0;
	width:95%
}

div.gallery div.video {
	cursor:pointer;
	display:inline-table;
	margin:0 10px 5px 0;
	width:150px
}

div.gallery div.video a.watch img {
	height:150px;
	width:267px;
}

div.gallery div.video a.watch img,
div.gallery div.video div.text {
	opacity:0.7
}

div.gallery div.video:hover a.watch img,
div.gallery div.video:hover div.text {
	opacity:1
}

div.gallery div.video a.watch,div.gallery div.video a.watch img {
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
	display:block;
	margin:0 auto;
	text-decoration:none
}

div.gallery div.video a:not(.soon) img {
	cursor:pointer
}

div.gallery div.video span.read_more {
	color:rgb(0,116,116);
	cursor:pointer;
	text-decoration:underline
}

div.gallery div.video span.syn {
	font-style: italic;
	margin: 0 auto;
	text-align: center;
	display: inline-block;
	width: 100%
}

div.gallery div.video span.syn_hidden {
	display:none
}

div.gallery div.video span.text > span.title {
	display:block
}

div.gallery div.video.film span.text > span.desc {
	display: none !important
}

div.gallery div.video span.text > span.title,div.gallery div.video:not(.film) span.text > span.desc {
	text-align:center
}

div.gallery div.video span.text span.title {
	font-size:13pt;
	font-weight:700;
	margin-top:5px
}

div.gallery div.video.film a.watch img {
	height:150px;
	width:106px
}

div.pageheader {
	display:none;
	font-size:30pt;
	margin:20px auto 0 10%
}

div.pageheader,div.filmography div.sectionheader,div.filmography ul li span {
	color:#E1C811
}

div#header div.headerbuttons a {
	-webkit-box-shadow: rgba(0,0,0,0.2) 0px 2px 3px, inset rgba(0,0,0,0.2) 0px -1px 2px;
	-khtml-box-shadow: rgba(0,0,0,0.2) 0px 2px 3px, inset rgba(0,0,0,0.2) 0px -1px 2px;
	-moz-box-shadow: rgba(0,0,0,0.2) 0px 2px 3px, inset rgba(0,0,0,0.2) 0px -1px 2px;
	-ms-box-shadow: rgba(0,0,0,0.2) 0px 2px 3px, inset rgba(0,0,0,0.2) 0px -1px 2px;
	-o-box-shadow: rgba(0,0,0,0.2) 0px 2px 3px, inset rgba(0,0,0,0.2) 0px -1px 2px;
	box-shadow: rgba(0,0,0,0.2) 0px 2px 3px, inset rgba(0,0,0,0.2) 0px -1px 2px;
	-webkit-border-radius: 2px;
	-khtml-border-radius: 2px;
	-moz-border-radius: 2px;
	-ms-border-radius: 2px;
	-o-border-radius: 2px;
	border-radius: 2px;
	height:23px;
	width:111px;
	background: #2f2f2f; /* Old browsers */
	background: -moz-radial-gradient(center, ellipse cover,  #2f2f2f 0%, #828c95 100%, #28343b 100%); /* FF3.6+ */
	background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#2f2f2f), color-stop(100%,#828c95), color-stop(100%,#28343b)); /* Chrome,Safari4+ */
	background: -webkit-radial-gradient(center, ellipse cover,  #2f2f2f 0%,#828c95 100%,#28343b 100%); /* Chrome10+,Safari5.1+ */
	background: -o-radial-gradient(center, ellipse cover,  #2f2f2f 0%,#828c95 100%,#28343b 100%); /* Opera 12+ */
	background: -ms-radial-gradient(center, ellipse cover,  #2f2f2f 0%,#828c95 100%,#28343b 100%); /* IE10+ */
	background: radial-gradient(ellipse at center,  #2f2f2f 0%,#828c95 100%,#28343b 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2f2f2f', endColorstr='#28343b',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
}

div#header div.headerbuttons a:hover,
div#header div.headerbuttons a.active {
	background: #828c95; /* Old browsers */
	background: -moz-radial-gradient(center, ellipse cover,  #828c95 0%, #2f2f2f 100%, #28343b 100%); /* FF3.6+ */
	background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#828c95), color-stop(100%,#828c95), color-stop(100%,#28343b)); /* Chrome,Safari4+ */
	background: -webkit-radial-gradient(center, ellipse cover,  #828c95 0%,#2f2f2f 100%,#28343b 100%); /* Chrome10+,Safari5.1+ */
	background: -o-radial-gradient(center, ellipse cover,  #828c95 0%,#2f2f2f 100%,#28343b 100%); /* Opera 12+ */
	background: -ms-radial-gradient(center, ellipse cover,  #828c95 0%,#2f2f2f 100%,#28343b 100%); /* IE10+ */
	background: radial-gradient(ellipse at center,  #828c95 0%,#2f2f2f 100%,#28343b 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2f2f2f', endColorstr='#28343b',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */	
}

ul.slider li img {
	height:360px;
	margin: 0 auto;
	-webkit-border-radius: 10px;
	-khtml-border-radius: 10px;
	-moz-border-radius: 10px;
	-ms-border-radius: 10px;
	-o-border-radius: 10px;
	border-radius: 10px
}