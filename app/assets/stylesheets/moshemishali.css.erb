body {
	font: 12pt "times new roman", "arial";
	color: Gray;
	background-color: #000;
	overflow-x: hidden;
}
div#main {
	margin: 0 auto
}

div.doctext.filmography {
	height: 270px
}

div.pageheader,
div.doctext.filmography div.sectionheader,
div.doctext.filmography ul li span {
	color: #E1C811
}

div.pageheader {
	font-size: 30pt;
	margin: 0 auto 0 10%
}

div.doctext.filmography div.sectionheader {
	font-size: 18pt;
	font-weight: bold;
	margin: 0;
}

div.doctext.filmography ul li span {
	font-size: 13pt;
	font-style: italic
}