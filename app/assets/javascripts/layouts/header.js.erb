$(document).ready(function(e){
	attachEvents();
  OnLoad();
});
attachEvents = function() {
	$(".headerbuttons a").on("click", function(e){
		hideAllButSection($(e.target).attr("goto").toLowerCase());
	});
};
hideAllButSection = function(sectionID) {
  $(".section").hide();
  $(".section#" + sectionID).show();
}